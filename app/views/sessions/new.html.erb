<div class="wrapper">

  <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "#{key}") %>
  <% end %>
  
  <h1>ろぐいん</h1>
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "ごー" %>
  <% end %>
</div>
